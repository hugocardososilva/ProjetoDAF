<%- model_class = Voto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:data) %>:</strong></dt>
  <dd><%= @voto.data %></dd>
  <dt><strong><%= model_class.human_attribute_name(:eleitor_id) %>:</strong></dt>
  <dd><%= @voto.eleitor_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:candidato_id) %>:</strong></dt>
  <dd><%= @voto.candidato_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              votos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_voto_path(@voto), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              voto_path(@voto),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
